import <seos_if_socket.camkes>;

import "../TlsRpcServer/TlsRpcServerInf.camkes";
import "../TlsRpcServer/SeosTlsRpcServerInf.camkes";

component TEST_TLS {
    control;

    // Networking
    uses        seos_if_socket      network_stack_rpc;
    dataport    Buf                 NwAppDataPort;
    consumes    ServiceReady        event_network_stack_init_done;

    // TLS
    uses        TlsRpcServerInf     TlsRpcServer;
    uses        SeosTlsRpcServerInf SeosTlsRpcServer;
    dataport    Buf                 tlsClientDataport;
}